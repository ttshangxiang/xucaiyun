{
  "shell": "assets/main.js",
  "entrypoint": "index.html",
  "fragments": ["assets/lazy-element.js"],
  "npm": true,
  "moduleResolution": "node",
  "sources": ["assets/**", "manifest/**", "manifest.json", "package.json"],
  "extraDependencies": [
    "robots.txt",
    "node_modules/@webcomponents/webcomponentsjs/**"
  ],
  "builds": [{
    "name": "production",
    "bundle": true,
    "js": { 
      "minify": true,
      "compile": "es5",
      "transformModulesToAmd": true
    },
    "addServiceWorker": true,
    "addPushManifest": true
  }]
}
